/*
Language: SPANISH (SPA)
Content: GP and LF information
Authors: Vanesa Vidal, Simon Mille

Some abbreviations used in the comments:

  DSyntAct = deep syntactic actant
  SSyntRel = surface syntactic relation
  V = verb
  N = noun
  Adj = adjective
  Adv = adverb
  Cop = copula
  Aux = auxiliary
  Pro = pronoun
  Det = determiner

*/

lexicon {

//==============================================================================================================================================
//                 DEFAULT ATTRIBUTES
//==============================================================================================================================================



// PREDICATE
// ---------
// By default, all predicates have a trivial diathesis.

predicate {
  person  =  3
  gp  =  { 1  =  I
         2  =  II
         3  =  III
         4  =  IV
         5  =  V
         6  =  VI }
}



//==============================================================================================================================================
//			VERBS 
//==============================================================================================================================================
// By default, the DSyntAct I of a verb becomes its subject.

verb : predicate {
  dpos = V
  spos = verb
  past_aux = haber_2
  pass_aux = ser_2
  progr_aux = estar_2
  gp = {
     I = {
        dpos = N
        rel = subj
     }
  }
  Magn = mucho_2
  AntiMagn = "un poco"
}


// VERB_DT
// -------
// Direct transitive verbs.
// By default, their DSyntAct II is their direct object.
// Direct transitive verbs can have more than two actants.

verb_dt : verb {                   // direct transitive
  gp = {
     II = {
        dpos = N
        rel = dobj

     }
  }
}

// VERB_IT
// -------
// Indirect transitive verbs.
// By default, their DSyntAct II is their indirect object.
// Indirect transitive verbs can have more than two actants.

verb_it : verb {                   // indirect transitive
  gp = {
     II = {
        dpos = N
        rel = obl_obj
        prep = a
     }
  }
}


// COPULA
// ------
// Copulas have a copulative relation to their DSyntAct II.

copula : verb {
//  cat = COP
  spos = copula
  gp = {
     II = {
        rel = copul
	dpos=N
     }
     II = {
        rel = copul
	dpos=A
     }
  }
}


// AUXILIARIES
// ---------
// Auxiliaries have passive/perfect/etc. analytical relations relation to their DSyntAct II.

auxiliary : verb {
//  cat = AUX
  spos = auxiliary
  gp = { II = { dpos = V } }	//CHECK THIS FOR I3MEDIA!!!!
}
modal : verb {
//  cat = AUX
  spos = modal_verb
  gp = { II = { rel = modal } }
}


//==============================================================================================================================================
//			NOUNS
//==============================================================================================================================================


// NOUN
// ----
// Common nouns.
// By default, all their DSyntAct are noun completives.

noun : predicate {
  dpos = N
  cat = N
  spos = noun
  countable = yes
  gp = {
     I = {
        rel = noun_compl	//CHECK THIS FOR I3MEDIA!!!! -> nominal_completive??
	prep = de
        dpos = N
     }
     II = {
        rel = noun_compl	//CHECK THIS FOR I3MEDIA!!!! -> nominal_completive??
	prep = de
        dpos = N
     }
     III = {
        rel = noun_compl	//CHECK THIS FOR I3MEDIA!!!! -> nominal_completive??
	prep = de
        dpos = N
     }
  }
}

// UNIT
// ----
// This is only for measurement units expressed as symbols.
// For example, "mg/m3", "l", "km", etc.

unit : noun {
  cat = UNIT
  spos = unit
  invariant = yes
}


// PROPER_NOUN
// -----------
// Proper nouns never have actants, but the formalism can't express this.
// They are never countable.

proper_noun : noun {
  spos = proper_noun
  countable = no
  invariant = yes
}


// TYPE_NUMBER
// -----------
// For digital representation of numbers

number : proper_noun {
  cat = NUM
  spos = number
}


// MASS_NOUN
// ---------
// Not a part of speech.
// Used to mark common nouns that are not countable.

mass_noun : noun {
  invariant = yes
  countable = no
  number = SG
}


// PRONOUN
// -------

pronoun : noun {
  spos = pronoun
}


// PERSONAL PRONOUN
// ----------------

personal_pronoun : pronoun {
  spos = personal_pronoun
}

COREF : personal_pronoun {}


//==============================================================================================================================================
//			ADJECTIVES
//==============================================================================================================================================

adjective_without_Oper1 : predicate {
  dpos = A
  cat = ADJ
  spos = adjective
  gp = {
    1 = ATTR
    ATTR = { dpos = N }
    gov = { dpos = N }
  }
  Magn = muy
  AntiMagn = relativamente
}

adjective : adjective_without_Oper1 {
  Oper1 = ser_1
}

adjective_part : adjective_without_Oper1 {
  gp  = {
    1 = II
    2 = ATTR
  }
  Oper2 = ser_1
}

determiner : adjective_without_Oper1 {
  spos = determiner
}

possessive : adjective_without_Oper1 {
  dpos = A
  spos = possessive
}

numeral : adjective_without_Oper1 { 	//CHECK FOR I3MEDIA-> DIFFERENCE WITH NUMBER?
  spos = numeral
}

//==============================================================================================================================================
//			ADVERBS 
//==============================================================================================================================================

adverb : predicate {
  dpos = Adv
  cat = ADV
  spos = adverb
  invariant = yes
  gp = {
     1 = ATTR
  }
  Magn = muy
  AntiMagn = relativamente
}

preposition : adverb {
  spos = preposition
  gp = {
     1 = ATTR
     2 = II
     3 = III
     II = {
        rel = prepos
        dpos = N
     }
     II = {		//temporarily disabled: the lex_standard rule is not adapted to restrictions on dpos (Ex: por culpa de expulsión/expulsar: we need mechanismes to control that); In addition, no all preps can govern a verb, can they?
        rel = prepos
        dpos = V
        finiteness=INF	//is this ok?
     }
  }
  Oper1 = ser_1  // seems to cause problems
}

conjunction : adverb {
  cat = CONJ
  spos = conjunction
  gp = { 1 = ATTR
         2 = II
         ATTR = { dpos = V }
         II = { dpos = V
                rel = sub_conj } }
}

coordination : adverb {
  spos = coord_conjunction
  gp = { 1 = COORD
         2 = II
         I = { rel = coord }
         II = { rel = coord_conj} }
}

connector : adverb {
  spos = connector
  gp = { 1 = "---"
         2 = ATTR }
}

_negation : adverb {
  spos = negative_adverb
}

