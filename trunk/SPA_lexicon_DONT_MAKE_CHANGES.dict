"a pesar" : conjunction { // "a pesar de"	//PATCH caus as a preposition I cannot get actant II to be a verb...
  gp = {
     1=II
     2=ATTR
     II = { prep = de               
            rel = obl_obj
            dpos=N }
     II = { prep = de               //PATCH: have to control better in the rules the introduction of an INF verb (for ex, not if actant1 is here)
            rel = obl_obj
            dpos=V
            finiteness=INF
            1=block}		//rule chechks if there is an actant one and prevents this pattern to be realized (Generalizable?)
  }
}
